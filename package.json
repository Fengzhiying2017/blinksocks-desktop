{
  "name": "blinksocks-desktop",
  "version": "0.0.1",
  "description": "Cross-platform desktop GUI for blinksocks",
  "main": "backend/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron ./backend",
    "pack": "electron-packager ./ blinksocks-desktop --platform=win32,linux --arch=x64,ia32 --overwrite --asar --ignore=/node_modules|ui/"
  },
  "dependencies": {
    "blinksocks": "^2.4.0-beta.4",
    "electron": "^1.6.6"
  },
  "devDependencies": {
    "cross-env": "^4.0.0",
    "electron-packager": "^8.6.0",
    "npm-run-all": "^4.0.2"
  },
  "author": "Micooz <micooz@hotmail.com>",
  "repository": {
    "url": "https://github.com/blinksocks/blinksocks-desktop",
    "type": "git"
  },
  "keywords": [
    "blinksocks",
    "blinksocks-desktop",
    "cross-platform",
    "gui"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 6"
  }
}