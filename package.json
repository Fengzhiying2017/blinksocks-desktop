{
  "name": "blinksocks-desktop",
  "version": "0.0.1",
  "description": "Cross-platform desktop GUI for blinksocks",
  "main": "main.js",
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "serve": "npm-run-all -p watch-css start-js",
    "start": "NODE_ENV=development electron .",
    "build": "react-scripts build",
    "pack": "electron-packager ./ blinksocks-desktop --platform=win32,linux --arch=x64,ia32 --overwrite --asar --ignore=/node_modules/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "blinksocks": "^2.4.0-beta.4",
    "blinksocks-utils": "^0.0.2",
    "electron": "^1.6.5",
    "material-ui": "^0.17.4",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-tap-event-plugin": "^2.0.1"
  },
  "devDependencies": {
    "electron-packager": "^8.6.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5"
  },
  "author": "Micooz <micooz@hotmail.com>",
  "repository": {
    "url": "https://github.com/blinksocks/blinksocks-desktop",
    "type": "git"
  },
  "keywords": [
    "blinksocks",
    "blinksocks-desktop",
    "cross-platform",
    "gui"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 6"
  }
}
